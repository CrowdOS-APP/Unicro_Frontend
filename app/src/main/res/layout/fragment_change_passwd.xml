<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/background3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/user_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/user_back2"
        android:layout_width="361dp"
        android:layout_height="126dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/button_type"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/blank1"
        android:layout_width="361dp"
        android:layout_height="500dp"
        android:layout_marginTop="176dp"
        android:background="@drawable/button_type"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/user_name3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/title"
        android:text="用户名"
        android:textColor="@color/black"
        android:textSize="24dp"
        app:layout_constraintStart_toStartOf="@+id/user_back2"
        app:layout_constraintTop_toTopOf="@+id/user_back2" />

    <TextView
        android:id="@+id/user_name4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/mainx"
        android:text="隐私-修改密码"
        android:textColor="@color/black"
        android:textSize="24dp"
        app:layout_constraintStart_toStartOf="@+id/blank1"
        app:layout_constraintTop_toTopOf="@+id/blank1" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="57dp"
        android:layout_height="53dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="36dp"
        android:src="@mipmap/user_2"
        app:layout_constraintStart_toStartOf="@+id/user_back2"
        app:layout_constraintTop_toTopOf="@+id/user_back2" />

    <TextView
        android:id="@+id/signature_blank3"
        android:layout_width="236dp"
        android:layout_height="59dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/button_type2"
        app:layout_constraintStart_toStartOf="@+id/user_back2"
        app:layout_constraintTop_toTopOf="@+id/user_back2" />

    <TextView
        android:id="@+id/email"
        android:layout_width="330dp"
        android:layout_height="58dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/button_type2"
        android:drawableLeft="@mipmap/email"
        android:fontFamily="@font/mainx"
        android:gravity="center_vertical"
        android:paddingLeft="11dp"
        android:textColorHint="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/blank1"
        app:layout_constraintStart_toStartOf="@+id/blank1"
        app:layout_constraintTop_toTopOf="@+id/blank1" />

    <TextView
        android:id="@+id/password"
        android:layout_width="330dp"
        android:layout_height="58dp"
        android:layout_marginTop="132dp"
        android:background="@drawable/button_type2"
        android:drawableLeft="@mipmap/key_1"
        android:fontFamily="@font/mainx"
        android:gravity="center_vertical"

        android:paddingLeft="4dp"

        android:textColor="@color/black"
        android:textColorHint="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/blank1"
        app:layout_constraintStart_toStartOf="@+id/blank1"
        app:layout_constraintTop_toTopOf="@+id/blank1" />

    <TextView
        android:id="@+id/password2"
        android:layout_width="330dp"
        android:layout_height="58dp"
        android:layout_marginTop="204dp"
        android:background="@drawable/button_type2"
        android:drawableLeft="@mipmap/lock"
        android:fontFamily="@font/mainx"
        android:gravity="center_vertical"
        android:paddingLeft="7dp"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/blank1"
        app:layout_constraintStart_toStartOf="@+id/blank1"
        app:layout_constraintTop_toTopOf="@+id/blank1" />


    <TextView
        android:id="@+id/password3"
        android:layout_width="330dp"
        android:layout_height="58dp"
        android:layout_marginTop="276dp"
        android:background="@drawable/button_type2"
        android:drawableLeft="@mipmap/lock"
        android:fontFamily="@font/mainx"
        android:gravity="center_vertical"
        android:paddingLeft="7dp"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/blank1"
        app:layout_constraintStart_toStartOf="@+id/blank1"
        app:layout_constraintTop_toTopOf="@+id/blank1" />

    <TextView
        android:id="@+id/password_change"
        android:layout_width="79dp"
        android:layout_height="42dp"
        android:layout_marginTop="444dp"
        android:background="@drawable/button_type2"
        android:fontFamily="@font/mainx"
        android:gravity="center"
        android:text="修改"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/blank1"
        app:layout_constraintStart_toStartOf="@+id/blank1"
        app:layout_constraintTop_toTopOf="@+id/blank1" />

    <TextView
        android:id="@+id/signature3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/mainx"
        android:text="很酷，不写个签。"
        android:textColor="@color/black"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@+id/signature_blank3"
        app:layout_constraintTop_toTopOf="@+id/signature_blank3" />

    <EditText
        android:id="@+id/private_change_password_email"
        android:layout_width="273dp"
        android:layout_height="35dp"
        android:layout_marginTop="12dp"
        android:background="@null"
        android:fontFamily="@font/mainx"
        android:gravity="center_vertical"
        android:hint="请输入你的邮箱"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:lines="1"
        android:maxLines="1"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="@+id/email"
        app:layout_constraintHorizontal_bias="0.754"
        app:layout_constraintStart_toStartOf="@+id/email"
        app:layout_constraintTop_toTopOf="@+id/email" />

    <EditText
        android:id="@+id/private_change_password_old"
        android:layout_width="230dp"
        android:layout_height="35dp"
        android:layout_marginTop="12dp"
        android:background="@null"
        android:fontFamily="@font/mainx"
        android:gravity="center_vertical"
        android:hint="请输入旧密码"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:lines="1"
        android:maxLines="1"
        android:password="true"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="@+id/password"
        app:layout_constraintHorizontal_bias="0.43"
        app:layout_constraintStart_toStartOf="@+id/password"
        app:layout_constraintTop_toTopOf="@+id/password" />

    <EditText
        android:id="@+id/private_change_password_new"
        android:layout_width="230dp"
        android:layout_height="35dp"
        android:layout_marginTop="12dp"
        android:background="@null"
        android:fontFamily="@font/mainx"
        android:gravity="center_vertical"
        android:hint="请输入新密码"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:lines="1"
        android:maxLines="1"
        android:password="true"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="@+id/password2"
        app:layout_constraintHorizontal_bias="0.44"
        app:layout_constraintStart_toStartOf="@+id/password2"
        app:layout_constraintTop_toTopOf="@+id/password2" />

    <EditText
        android:id="@+id/private_change_password_new2"
        android:layout_width="230dp"
        android:layout_height="35dp"
        android:layout_marginTop="84dp"
        android:background="@null"
        android:fontFamily="@font/mainx"
        android:gravity="center_vertical"
        android:hint="请再次输入新密码"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:lines="1"
        android:maxLines="1"
        android:password="true"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="@+id/password2"
        app:layout_constraintHorizontal_bias="0.43"
        app:layout_constraintStart_toStartOf="@+id/password2"
        app:layout_constraintTop_toTopOf="@+id/password2" />

    <ImageView
        android:id="@+id/hide_password_old"
        android:layout_width="35dp"
        android:layout_height="32dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="12dp"
        android:src="@drawable/icon_eye"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="@+id/password"
        app:layout_constraintTop_toTopOf="@+id/password" />

    <ImageView
        android:id="@+id/hide_password_new"
        android:layout_width="35dp"
        android:layout_height="32dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="12dp"
        android:clickable="true"
        android:src="@drawable/icon_eye"
        app:layout_constraintEnd_toEndOf="@+id/password2"
        app:layout_constraintTop_toTopOf="@+id/password2" />

    <ImageView
        android:id="@+id/hide_password_new2"
        android:layout_width="35dp"
        android:layout_height="32dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="12dp"
        android:clickable="true"
        android:src="@drawable/icon_eye"
        app:layout_constraintEnd_toEndOf="@+id/password2"
        app:layout_constraintTop_toTopOf="@+id/password2" />

</androidx.constraintlayout.widget.ConstraintLayout>