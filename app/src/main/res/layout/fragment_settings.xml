<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/background3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/user_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/user_back2"
        android:layout_width="361dp"
        android:layout_height="126dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/button_type"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/blank1"
        android:layout_width="361dp"
        android:layout_height="500dp"
        android:layout_marginTop="176dp"
        android:background="@drawable/button_type"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/user_name3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/title"
        android:text="用户名"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/user_back2"
        app:layout_constraintTop_toTopOf="@+id/user_back2" />

    <TextView
        android:id="@+id/user_name4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/mainx"
        android:text="隐私-用户设置"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/blank1"
        app:layout_constraintTop_toTopOf="@+id/blank1" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="57dp"
        android:layout_height="53dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="36dp"
        android:src="@mipmap/user_2"
        app:layout_constraintStart_toStartOf="@+id/user_back2"
        app:layout_constraintTop_toTopOf="@+id/user_back2" />

    <TextView
        android:id="@+id/signature_blank3"
        android:layout_width="236dp"
        android:layout_height="59dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/button_type2"
        app:layout_constraintStart_toStartOf="@+id/user_back2"
        app:layout_constraintTop_toTopOf="@+id/user_back2" />

    <TextView
        android:id="@+id/signature3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/mainx"
        android:text="很酷，不写个签。"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@+id/signature_blank3"
        app:layout_constraintTop_toTopOf="@+id/signature_blank3" />

    <TextView
        android:id="@+id/email2"
        android:layout_width="330dp"
        android:layout_height="58dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/button_type2"
        android:fontFamily="@font/mainx"
        android:gravity="center_vertical"
        android:paddingLeft="11dp"
        android:textColorHint="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/blank1"
        app:layout_constraintStart_toStartOf="@+id/blank1"
        app:layout_constraintTop_toTopOf="@+id/blank1" />

    <TextView
        android:id="@+id/email3"
        android:layout_width="330dp"
        android:layout_height="130dp"
        android:layout_marginTop="132dp"
        android:background="@drawable/button_type2"
        android:fontFamily="@font/mainx"
        android:gravity="center_vertical"
        android:paddingLeft="11dp"
        android:textColorHint="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/blank1"
        app:layout_constraintStart_toStartOf="@+id/blank1"
        app:layout_constraintTop_toTopOf="@+id/blank1" />

    <TextView
        android:id="@+id/email4"
        android:layout_width="330dp"
        android:layout_height="160dp"
        android:layout_marginTop="272dp"
        android:background="@drawable/button_type2"
        android:fontFamily="@font/mainx"
        android:gravity="center_vertical"
        android:paddingLeft="11dp"
        android:textColorHint="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/blank1"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="@+id/blank1"
        app:layout_constraintTop_toTopOf="@+id/blank1" />

    <EditText
        android:id="@+id/private_settings_user"
        android:layout_width="230dp"
        android:layout_height="34dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="12dp"
        android:background="@null"
        android:fontFamily="@font/main"
        android:gravity="center_vertical"
        android:hint="想要取一个酷酷的名字吗XD"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:lines="1"
        android:maxLines="1"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/email2"
        app:layout_constraintTop_toTopOf="@+id/email2" />

    <TextView
        android:id="@+id/password_change2"
        android:layout_width="79dp"
        android:layout_height="42dp"
        android:layout_marginTop="444dp"
        android:background="@drawable/button_type2"
        android:fontFamily="@font/mainx"
        android:gravity="center"
        android:text="修改"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/blank1"
        app:layout_constraintStart_toStartOf="@+id/blank1"
        app:layout_constraintTop_toTopOf="@+id/blank1" />

    <TextView
        android:id="@+id/user_name2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:fontFamily="@font/mainx"
        android:text="用户名"
        android:textColor="@color/orange_main"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/email2"
        app:layout_constraintStart_toStartOf="@+id/email2"
        app:layout_constraintTop_toTopOf="@+id/email2" />

    <TextView
        android:id="@+id/user_name5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:fontFamily="@font/mainx"
        android:text="个性签名"
        android:textColor="@color/orange_main"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/email3"
        app:layout_constraintStart_toStartOf="@+id/email3"
        app:layout_constraintTop_toTopOf="@+id/email3"
        app:layout_constraintVertical_bias="0.095" />

    <TextView
        android:id="@+id/user_name6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/mainx"
        android:text="头像"
        android:textColor="@color/orange_main"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/email4"
        app:layout_constraintStart_toStartOf="@+id/email4"
        app:layout_constraintTop_toTopOf="@+id/email4"
        app:layout_constraintVertical_bias="0.095" />

    <EditText
        android:id="@+id/change_signature"
        android:layout_width="310dp"
        android:layout_height="80dp"
        android:layout_marginTop="40dp"
        android:background="@null"
        android:fontFamily="@font/mainx"
        android:gravity="start"
        android:hint="再写一段酷酷的个性签名XD"
        android:inputType="textMultiLine"
        android:lineSpacingExtra="8dp"
        android:lineSpacingMultiplier="1.2"
        android:scrollbars="vertical"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="@+id/email3"
        app:layout_constraintStart_toStartOf="@+id/email3"
        app:layout_constraintTop_toTopOf="@+id/email3" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="27dp"
        android:layout_height="28dp"
        android:layout_marginStart="300dp"
        android:fontFamily="@font/title"
        android:gravity="center_horizontal|center_vertical"
        android:text="15"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/email2"
        app:layout_constraintStart_toStartOf="@+id/email2"
        app:layout_constraintTop_toTopOf="@+id/email2"
        app:layout_constraintVertical_bias="0.566" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="27dp"
        android:layout_height="28dp"
        android:layout_marginStart="216dp"
        android:fontFamily="@font/title"
        android:gravity="center_horizontal|center_vertical"
        android:text="80"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/email3"
        app:layout_constraintStart_toEndOf="@+id/user_name5"
        app:layout_constraintTop_toTopOf="@+id/email3"
        app:layout_constraintVertical_bias="0.065" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/sculpture"
        android:layout_width="314dp"
        android:layout_height="110dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="@+id/email4"
        app:layout_constraintStart_toStartOf="@+id/email4"
        app:layout_constraintTop_toTopOf="@+id/email4" />

</androidx.constraintlayout.widget.ConstraintLayout>