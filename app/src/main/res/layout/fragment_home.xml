<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="550dp"
        android:layout_height="550dp"
        android:layout_marginBottom="416dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/button_type_circle_main" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="500dp"
        android:layout_height="500dp"
        android:layout_marginBottom="436dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/button_type_circle_main2" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/scan"
        tools:ignore="SpeakableTextPresentCheck" />

    <com.baidu.mapapi.map.MapView
        android:id="@+id/mapView2"
        android:layout_width="364dp"
        android:layout_height="530dp"
        android:background="@drawable/button_type"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.646"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="291dp"
        android:layout_height="54dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/button_type"
        android:drawableLeft="@mipmap/search"
        android:ems="10"
        android:fontFamily="@font/mainx"

        android:gravity="center_vertical"
        android:inputType="text"
        android:paddingLeft="12dp"
        android:text="@string/Null"
        android:textSize="16dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView10"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Upload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="@+id/mapView2"
        app:layout_constraintEnd_toEndOf="@+id/mapView2"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/mapView2"
        app:layout_constraintTop_toTopOf="@+id/mapView2"
        app:layout_constraintVertical_bias="0.966"
        app:srcCompat="@mipmap/cloud"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/home_search"
        android:layout_width="222dp"
        android:layout_height="37dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="8dp"
        android:background="@null"
        android:fontFamily="@font/mainx"
        android:gravity="center_vertical"
        android:hint="搜索功能正在开发中"
        android:imeOptions="actionDone"
        android:lines="1"
        android:maxLines="1"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="16dp"
        app:layout_constraintHorizontal_bias="0.754"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <TextView
        android:id="@+id/location"
        android:layout_width="222dp"
        android:layout_height="37dp"
        android:background="@null"
        android:fontFamily="@font/mainx"
        android:gravity="center|center_vertical"
        android:textColor="@color/yellow_light_1"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/mapView2"
        app:layout_constraintEnd_toEndOf="@+id/mapView2"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="@+id/mapView2"
        tools:text="经纬度" />

</androidx.constraintlayout.widget.ConstraintLayout>